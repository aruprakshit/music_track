<%= form_tag fetch_path  do %>
  <p> Label1: <%= text_field_tag 'label1' %> </p>
  <p> Label2: <%= text_field_tag 'label2' %> </p>
  <p> Label3: <%= text_field_tag 'label3' %> </p>
  <p> Start Date: <%= date_field_tag 'start_date' %> </p>
  <p> End Date:   <%= date_field_tag 'end_date' %> </p>
  <p> Email: <%= email_field_tag 'email' %> </p>
  <p> <%= submit_tag 'Fetch' %> </p>
<% end %>
